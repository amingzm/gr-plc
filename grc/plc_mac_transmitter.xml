<block>
  <name>MAC Transmitter</name>
  <key>plc_mac_transmitter</key>
  <category>PLC</category>
  <import>import plc</import>
  <make>plc.mac_transmitter($input_filename, $period_ms, $debug)</make>
  <param>
    <name>File</name>
    <key>input_filename</key>
    <value></value>
    <type>file_open</type>
  </param>
  <param>
    <name>Period (ms)</name>
    <key>period_ms</key>
    <type>float</type>
  </param>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Enable</name>
      <key>True</key>
    </option>
    <option>
      <name>Disable</name>
      <key>False</key>
    </option>
  </param>
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
