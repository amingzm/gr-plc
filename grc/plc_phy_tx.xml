<?xml version="1.0"?>
<block>
  <name>Phy Tx</name>
  <key>plc_phy_tx</key>
  <category>PLC</category>
  <import>import plc</import>
  <make>plc.phy_tx($debug)</make>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Enable</name>
      <key>True</key>
    </option>
    <option>
      <name>Disable</name>
      <key>False</key>
    </option>
  </param>
  <sink>
    <name>mac in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>mac out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
