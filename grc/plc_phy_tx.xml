<?xml version="1.0"?>
<block>
  <name>Phy Tx</name>
  <key>plc_phy_tx</key>
  <category>PLC</category>
  <import>import plc</import>
  <make>plc.phy_tx($log)</make>
  <param>
    <name>Log</name>
    <key>log</key>
    <value>0</value>
    <type>int</type>
    <option>
      <name>Disabled</name>
      <key>0</key>
    </option>
    <option>
      <name>Info</name>
      <key>1</key>
    </option>
    <option>
      <name>Debug</name>
      <key>2</key>
    </option>
    <option>
      <name>Debug Level 2</name>
      <key>3</key>
    </option>
  </param>
  <sink>
    <name>mac in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>mac out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
