<block>
  <name>MAC Receiver</name>
  <key>plc_mac_receiver</key>
  <category>PLC</category>
  <import>import plc</import>
  <make>plc.mac_receiver($output_filename, $debug)</make>
  <param>
    <name>File</name>
    <key>output_filename</key>
    <value></value>
    <type>file_save</type>
  </param>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Enable</name>
      <key>True</key>
    </option>
    <option>
      <name>Disable</name>
      <key>False</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>
</block>
