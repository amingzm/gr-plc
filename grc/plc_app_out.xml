<?xml version="1.0"?>
<block>
  <name>APP Out</name>
  <key>plc_app_out</key>
  <category>PLC</category>
  <import>import plc</import>
  <make>plc.app_out($dest, $debug)</make>
  <param>
    <name>Destination</name>
    <key>dest</key>
    <value>[0x23, 0x23, 0x23, 0x23, 0x23, 0x23]</value>
    <type>int_vector</type>
  </param>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Enable</name>
      <key>True</key>
    </option>
    <option>
      <name>Disable</name>
      <key>False</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>mac out</name>
    <type>message</type>
  </source>
</block>
