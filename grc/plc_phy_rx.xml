<?xml version="1.0"?>
<block>
  <name>Phy Rx</name>
  <key>plc_phy_rx</key>
  <category>PLC</category>
  <import>import plc</import>
  <make>plc.phy_rx($info, $debug)</make>
 <param>
    <name>Info</name>
    <key>info</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Enable</name>
      <key>True</key>
    </option>
    <option>
      <name>Disable</name>
      <key>False</key>
    </option>
  </param>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>int</type>
    <option>
      <name>Disable</name>
      <key>0</key>
    </option>
    <option>
      <name>Enable</name>
      <key>1</key>
    </option>
    <option>
      <name>Verbose</name>
      <key>2</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <sink>
    <name>mac in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>mac out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
