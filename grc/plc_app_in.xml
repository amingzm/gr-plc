<?xml version="1.0"?>
<block>
  <name>APP In</name>
  <key>plc_app_in</key>
  <category>PLC</category>
  <import>import plc</import>
  <make>plc.app_in($log)</make>
  <param>
    <name>Log</name>
    <key>log</key>
    <value>0</value>
    <type>int</type>
    <option>
      <name>Notice</name>
      <key>0</key>
    </option>
    <option>
      <name>Info</name>
      <key>1</key>
    </option>
    <option>
      <name>Debug</name>
      <key>2</key>
    </option>
  </param>
  <sink>
    <name>mac in</name>
    <type>message</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
