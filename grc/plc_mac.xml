<?xml version="1.0"?>
<block>
  <name>MAC</name>
  <key>plc_mac</key>
  <category>PLC</category>
  <import>import plc</import>
  <make>plc.mac($device_addr, $master, $tmi, $dest_addr, $broadcast_tone_mask, $sync_tone_mask, $force_tone_mask, $target_ber, $channel_est_mode, $info, $debug)</make>
  <param>
    <name>Address</name>
    <key>device_addr</key>
    <value>[0x23, 0x23, 0x23, 0x23, 0x23, 0x23]</value>
    <type>int_vector</type>
  </param>
  <param>
    <name>Role</name>
    <key>master</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Master</name>
      <key>True</key>
    </option>
    <option>
      <name>Slave</name>
      <key>False</key>
    </option>
  </param>
  <param>
    <name>Dest address</name>
    <key>dest_addr</key>
    <value>[0x45, 0x45, 0x45, 0x45, 0x45, 0x45]</value>
    <type>int_vector</type>
  </param>
  <param>
    <name>Tone Mode</name>
    <key>tmi</key>
    <value>0</value>
    <type>int</type>
    <option>
      <name>STD_ROBO</name>
      <key>0</key>
    </option>
    <option>
      <name>HS_ROBO</name>
      <key>1</key>
    </option>
    <option>
      <name>MINI_ROBO</name>
      <key>2</key>
    </option>
    <option>
      <name>NO ROBO</name>
      <key>3</key>
    </option>
  </param>
  <param>
    <name>Broadcast Mask</name>
    <key>broadcast_tone_mask</key>
    <value></value>
    <type>int_vector</type>
  </param>
  <param>
    <name>Sync Mask</name>
    <key>sync_tone_mask</key>
    <value></value>
    <type>int_vector</type>
  </param>
  <param>
    <name>Force Tone Mask</name>
    <key>force_tone_mask</key>
    <value>[]</value>
    <type>int_vector</type>
  </param>
  <param>
    <name>Target BER</name>
    <key>target_ber</key>
    <value></value>
    <type>float</type>
  </param>
  <param>
    <name>Channel Estimation</name>
    <key>channel_est_mode</key>
    <value>0</value>
    <type>int</type>
    <option>
      <name>SOUND</name>
      <key>0</key>
    </option>
    <option>
      <name>PAYLOAD</name>
      <key>1</key>
    </option>
    <option>
      <name>PREAMBLE</name>
      <key>2</key>
    </option>
  </param>
  <param>
    <name>Info</name>
    <key>info</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Enable</name>
      <key>True</key>
    </option>
    <option>
      <name>Disable</name>
      <key>False</key>
    </option>
  </param>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    <option>
      <name>Enable</name>
      <key>True</key>
    </option>
    <option>
      <name>Disable</name>
      <key>False</key>
    </option>
  </param>
  <sink>
    <name>app in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <sink>
    <name>phy in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>app out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
  <source>
    <name>phy out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
